{
  "schemaVersion": 1,
  "id": "mobfarmutilities",
  "version": "${version}",
  "name": "Mob Farm Helpers",
  "description": "Add some useful mob farm utilities",
  "authors": [
    "6LeoMC"
  ],
  "contact": {
    "issues": "https://github.com/yzl210/MobFarmUtilities/issues",
    "sources": "https://github.com/yzl210/MobFarmUtilities"
  },
  "license": "GPLv3",
  "environment": "*",
  "mixins": [
    "mobfarmutilities.mixins.json"
  ],
  "entrypoints": {
    "main": [
      "cn.leomc.mobfarmutilities.fabric.MobFarmUtilitiesFabric"
    ],
    "client": [
      "cn.leomc.mobfarmutilities.fabric.MobFarmUtilitiesFabricClient"
    ]
  },
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": ">=1.16.3",
    "architectury": ">=1.8.130"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "waila:plugins": {
      "id": "mobfarmutilities:wthit_plugin",
      "initializer": "cn.leomc.mobfarmutilities.fabric.compat.WTHITCompat"
    }
  }
}